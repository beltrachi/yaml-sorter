{
  "name": "yaml-sorter",
  "displayName": "yaml-sorter",
  "description": "Sort yml keys",
  "version": "0.0.1",
  "publisher": "beltrachi",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:yaml-sorter.sortYamlKeys"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yaml-sorter.sortYamlKeys",
        "title": "Sort yaml keys",
        "when": "editorHasSelection && !editorReadOnly"
      }
    ]
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/beltrachi/yaml-sorter.git"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^10.8.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
